{
  "name": "one_rail",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "private": true,
  "keywords": [],
  "author": "Mateusz Å»upa",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.28.1",
  "scripts": {
    "build": "npm run clean && eslint && tsc && tsc-alias",
    "dev": "concurrently \"npm run watch:ts\" \"npm run watch:node\"",
    "start:prod": "node dist/src/main.js",
    "clean": "rm -rf dist",
    "watch:ts": "tsc --watch",
    "watch:node": "nodemon --watch dist --ext js --exec \"tsc-alias && node dist/src/main.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --runTestsByPath test/unit",
    "test:coverage": "jest --coverage",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "preinstall": "[[ $CI != true ]] && . ~/.nvm/nvm.sh && nvm use || true",
    "prepare": "husky",
    "compose": "docker compose -f compose.yml up -d",
    "compose-down": "docker compose -f compose.yml down",
    "migrate:up": "sequelize-cli db:migrate --config config/config.js",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "docker-build": "./build.sh",
    "seed": "ts-node -r tsconfig-paths/register seeders/seed.ts",
    "seed:prod": "node dist/seeders/seed.js"
  },
  "dependencies": {
    "@faker-js/faker": "^10.2.0",
    "@keyv/redis": "^5.1.6",
    "@types/cors": "^2.8.19",
    "@types/swagger-jsdoc": "^6.0.4",
    "chalk": "^5.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "class-validator-jsonschema": "^5.1.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "keyv": "^5.6.0",
    "mysql2": "^3.16.1",
    "nest-winston": "^1.10.2",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.2",
    "@types/express": "latest",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@types/sequelize": "^6.12.0",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/parser": "^8.53.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.0",
    "sequelize-cli": "^6.6.5",
    "ts-jest": "^29.4.6",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  }
}